{
	"name": "TrackOurSpends",
	"version": "11.17.6",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"build-android": "cd android/ && ./gradlew assembleRelease && cd ../",
		"preinstall": "yarn run uninstall",
		"install": "adb install android/app/build/outputs/apk/app-release.apk",
		"preinstall-apk": "yarn run uninstall",
		"install-apk": "adb install ~/Downloads/6.apk",
		"uninstall": "adb uninstall com.trackourspends",
		"clean-gradle": "cd android/ && ./gradlew clean",
		"postclean-gradle": "cd ~/repos/TrackOurSpends",
		"test": "jest"
	},
	"dependencies": {
		"babel-plugin-module-resolver": "^2.7.1",
		"react": "16.0.0",
		"react-native": "0.50.1",
		"react-native-device-info": "^0.12.1",
		"react-native-google-signin": "^0.12.0",
		"react-native-vector-icons": "^4.4.2",
		"react-navigation": "^1.0.0-beta.13",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"redux-thunk": "^2.2.0"
	},
	"devDependencies": {
		"babel-jest": "21.2.0",
		"babel-preset-react-native": "4.0.0",
		"jest": "21.2.1",
		"react-test-renderer": "16.0.0"
	},
	"jest": {
		"preset": "react-native"
	}
}
